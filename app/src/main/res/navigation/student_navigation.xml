<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/student_navigation"
    app:startDestination="@id/studentHomeFragment">

    <fragment
        android:id="@+id/studentHomeFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.StudentHomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_student_home">
        <action
            android:id="@+id/action_studentHomeFragment_to_studentLoginFragment2"
            app:destination="@id/studentLoginFragment2" />
        <action
            android:id="@+id/action_studentHomeFragment_to_studentCoursePage"
            app:destination="@id/studentCoursePage" />
        <action
            android:id="@+id/action_studentHomeFragment_to_aboutUsFragment"
            app:destination="@id/aboutUsFragment" />
        <action
            android:id="@+id/action_studentHomeFragment_to_settingsFragment"
            app:destination="@id/settingsFragment3" />
        <action
            android:id="@+id/action_studentHomeFragment_to_supportFragment"
            app:destination="@id/supportFragment" />
        <action
            android:id="@+id/action_studentHomeFragment_to_settingsFragment3"
            app:destination="@id/settingsFragment3" />
        <action
            android:id="@+id/action_studentHomeFragment_to_studentAppointmentDisplayFragment"
            app:destination="@id/studentAppointmentDisplayFragment" />
        <action
            android:id="@+id/action_studentHomeFragment_to_tutorCourseSelection"
            app:destination="@id/tutorCourseSelection" />
        <action
            android:id="@+id/action_studentHomeFragment_to_studentDetailsFragment"
            app:destination="@id/studentDetailsFragment" />
        <action
            android:id="@+id/action_studentHomeFragment_to_homeFragment2"
            app:destination="@id/homeFragment2" />
    </fragment>
    <fragment
        android:id="@+id/studentDetailsFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.StudentDetailsFragment"
        android:label="Details"
        tools:layout="@layout/fragment_details">
        <action
            android:id="@+id/action_studentDetailsFragment_to_studentHomeFragment"
            app:destination="@id/studentHomeFragment" />
        <action
            android:id="@+id/action_studentDetailsFragment_self"
            app:destination="@id/studentDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/studentLoginFragment2"
        android:name="sheridancollege.proWarriors.StudentFragments.StudentLoginFragment"
        android:label="Student Login"
        tools:layout="@layout/fragment_student_login" />
    <fragment
        android:id="@+id/studentTutorDescriptionFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.StudentTutorDescriptionFragment"
        android:label="Tutor Details"
        tools:layout="@layout/fragment_student_tutor_description">
        <action
            android:id="@+id/action_studentTutorDescriptionFragment_to_studentAppointmentBookingFragment"
            app:destination="@id/studentAppointmentBookingFragment" />
        <action
            android:id="@+id/action_studentTutorDescriptionFragment_to_tutorReviewFragment"
            app:destination="@id/tutorReviewFragment" />
        <action
            android:id="@+id/action_studentTutorDescriptionFragment_to_studentAppointmentDisplayFragment"
            app:destination="@id/studentAppointmentDisplayFragment" />
    </fragment>
    <fragment
        android:id="@+id/studentAppointmentBookingFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.StudentAppointmentBookingFragment"
        android:label="Appointment Booking"
        tools:layout="@layout/fragment_student_appointment_booking">
        <action
            android:id="@+id/action_studentAppointmentBookingFragment_to_bookingConfirmationFragment"
            app:destination="@id/bookingConfirmationFragment" />
    </fragment>
    <fragment
        android:id="@+id/studentCoursePage"
        android:name="sheridancollege.proWarriors.StudentFragments.StudentCoursePage"
        android:label="Course List"
        tools:layout="@layout/fragment_student_course_page">
        <action
            android:id="@+id/action_studentCoursePage_to_courseDetailsFragment"
            app:destination="@id/courseDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/courseDetailsFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.CourseDetailsFragment"
        android:label="Course Details"
        tools:layout="@layout/fragment_course_details">
        <action
            android:id="@+id/action_courseDetailsFragment_to_studentTutorDescriptionFragment"
            app:destination="@id/studentTutorDescriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/aboutUsFragment"
        android:name="sheridancollege.proWarriors.Home.AboutUsFragment"
        android:label="About Us"
        tools:layout="@layout/fragment_about_us" />
        android:id="@+id/settingsFragment"
        android:name="sheridancollege.proWarriors.Settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/supportFragment"
        android:name="sheridancollege.proWarriors.Support.SupportFragment"
        android:label="Support"
        tools:layout="@layout/fragment_support" />
    <fragment
        android:id="@+id/settingsFragment3"
        android:name="sheridancollege.proWarriors.Settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/tutorReviewFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.TutorReviewFragment"
        android:label="Tutor Review"
        tools:layout="@layout/fragment_tutor_review">
        <action
            android:id="@+id/action_tutorReviewFragment_to_studentTutorDescriptionFragment"
            app:destination="@id/studentTutorDescriptionFragment" />
    </fragment>
    <fragment
        android:id="@+id/studentAppointmentDisplayFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.StudentAppointmentDisplayFragment"
        android:label="Appointments"
        tools:layout="@layout/fragment_student_appointment_display" />
    <fragment
        android:id="@+id/tutorCourseSelection"
        android:name="sheridancollege.proWarriors.TutorFragments.TutorCourseSelection"
        android:label="fragment_tutor_course_selection"
        tools:layout="@layout/fragment_tutor_course_selection" >
        <action
            android:id="@+id/action_tutorCourseSelection_to_tutorApplicationFragment2"
            app:destination="@id/tutorApplicationFragment2" />
    </fragment>
    <fragment
        android:id="@+id/tutorApplicationFragment2"
        android:name="sheridancollege.proWarriors.TutorFragments.TutorApplicationFragment"
        android:label="fragment_tutor_application"
        tools:layout="@layout/fragment_tutor_application" >
        <action
            android:id="@+id/action_tutorApplicationFragment2_to_tutorAvailabilityFragment3"
            app:destination="@id/tutorAvailabilityFragment3" />
    </fragment>
    <fragment
        android:id="@+id/tutorAvailabilityFragment3"
        android:name="sheridancollege.proWarriors.TutorFragments.TutorAvailabilityFragment"
        android:label="fragment_tutor_availability"
        tools:layout="@layout/fragment_tutor_availability" >
        <action
            android:id="@+id/action_tutorAvailabilityFragment3_to_studentHomeFragment"
            app:destination="@id/studentHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/bookingConfirmationFragment"
        android:name="sheridancollege.proWarriors.StudentFragments.BookingConfirmationFragment"
        android:label="Confirmation Page"
        tools:layout="@layout/fragment_booking_confirmation">
        <action
            android:id="@+id/action_bookingConfirmationFragment_to_studentHomeFragment"
            app:destination="@id/studentHomeFragment" />
    </fragment>
    <fragment
        android:id="@+id/homeFragment2"
        android:name="sheridancollege.proWarriors.Home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />
</navigation>